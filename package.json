{
  "name": "jsonx",
  "version": "1.10.3",
  "description": "React JSON Syntax - Construct React elements, JSX and HTML with JSON without transpilers. JSONX supports React Function and Class Components, React Lazy and Suspense Components, and full support for React Hooks",
  "main": "dist/index.cjs.js",
  "module": "build/index.js",
  "esm": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "web": "dist/index.web.js",
  "modules.root": "build",
  "type": "module",
  "scripts": {
    "dev": "rollup -c -w",
    "benchmark": "node -r esm  benchmark/main_bmrk.js",
    "test": "jest --coverage",
    "doc": "tsc && typedoc && sitedown manual/ -b docs/manual",
    "compile": "tsc",
    "build": "tsc && rollup -c && typedoc && sitedown manual/ -b docs/manual",
    "coverage": "coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/repetere/jsonx.git"
  },
  "keywords": [
    "jsx",
    "React",
    "server-side-react",
    "json jsx",
    "jamstack",
    "json",
    "jsonx",
    "reactive",
    "react-json",
    "react-json-schema",
    "json-react",
    "jsx-json"
  ],
  "author": {
    "name": "Yaw Joseph Etse",
    "email": "yaw@repetere.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/repetere/jsonx/issues"
  },
  "homepage": "https://github.com/repetere/jsonx#readme",
  "devDependencies": {
    "@commitlint/cli": "16.0.0",
    "@commitlint/config-conventional": "16.0.0",
    "@repetere/esdoc-inject-gtm-plugin": "0.1.0",
    "@rollup/plugin-alias": "3.1.8",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.1",
    "@rollup/plugin-replace": "3.0.0",
    "@rollup/plugin-sucrase": "4.0.1",
    "@rollup/plugin-typescript": "8.3.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^12.0.0",
    "@types/chai": "4.3.0",
    "@types/jest": "^27.0.1",
    "@types/jsdom-global": "3.0.2",
    "@types/luxon": "2.0.8",
    "@types/memory-cache": "0.2.1",
    "@types/numeral": "2.0.2",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-dom-factories": "1.0.2",
    "@types/sinon": "10.0.6",
    "@types/ua-parser-js": "0.7.36",
    "benchmark": "2.1.4",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "coveralls": "3.1.1",
    "husky": "7.0.4",
    "install": "0.13.0",
    "jest": "^27.0.6",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "lodash-es": "4.17.21",
    "luxon": "2.2.0",
    "microtime": "3.0.0",
    "prettier": "2.5.1",
    "puppeteer": "13.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "^2.53.1",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-terser-js": "1.0.2",
    "routes": "2.1.0",
    "sinon": "12.0.1",
    "sinon-chai": "3.7.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.2.1",
    "tsd-jsdoc": "2.5.0",
    "tslib": "^2.3.1",
    "tslint-config-prettier": "1.18.0",
    "typedoc": "^0.22.3",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.0",
    "create-react-class": "^15.7.0",
    "memory-cache": "0.2.0",
    "numeral": "2.0.6",
    "react-dom-factories": "1.0.2",
    "react-hook-form": "^7.3.4",
    "ua-parser-js": "^1.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "./src/jest_custom_test_env.ts"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "Release: ${nextRelease.version}\n\n${nextRelease.notes}"
      }
    ],
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/changelog"
    ]
  }
}
