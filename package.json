{
  "name": "jsonx",
  "version": "1.8.1",
  "description": "React JSON Syntax - Construct React elements, JSX and HTML with JSON without transpilers. JSONX supports React Function and Class Components, React Lazy and Suspense Components, and full support for React Hooks",
  "main": "dist/index.cjs.js",
  "module": "build/index.js",
  "esm": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "web": "dist/index.web.js",
  "modules.root": "build",
  "type": "module",
  "scripts": {
    "dev": "rollup -c -w",
    "benchmark": "node -r esm  benchmark/main_bmrk.js",
    "test": "jest --coverage",
    "doc": "tsc && typedoc && sitedown manual/ -b docs/manual",
    "compile": "tsc",
    "build": "tsc && rollup -c && typedoc && sitedown manual/ -b docs/manual",
    "coverage": "coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/repetere/jsonx.git"
  },
  "keywords": [
    "jsx",
    "React",
    "server-side-react",
    "json jsx",
    "jamstack",
    "json",
    "jsonx",
    "reactive",
    "react-json",
    "react-json-schema",
    "json-react",
    "jsx-json"
  ],
  "author": {
    "name": "Yaw Joseph Etse",
    "email": "yaw@repetere.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/repetere/jsonx/issues"
  },
  "homepage": "https://github.com/repetere/jsonx#readme",
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@repetere/esdoc-inject-gtm-plugin": "0.1.0",
    "@rollup/plugin-alias": "3.1.2",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-replace": "2.4.1",
    "@rollup/plugin-sucrase": "3.1.0",
    "@rollup/plugin-typescript": "8.2.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.10",
    "@semantic-release/release-notes-generator": "9.0.2",
    "@types/chai": "4.2.15",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.21",
    "@types/jsdom-global": "3.0.2",
    "@types/luxon": "1.26.2",
    "@types/memory-cache": "0.2.1",
    "@types/numeral": "2.0.1",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-dom-factories": "1.0.2",
    "@types/sinon": "9.0.11",
    "@types/ua-parser-js": "0.7.35",
    "@types/use-global-hook": "0.1.3",
    "benchmark": "2.1.4",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "coveralls": "3.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-adapter-react-16.3": "1.7.4",
    "husky": "5.2.0",
    "install": "0.13.0",
    "jest": "26.6.3",
    "jsdom": "16.5.1",
    "jsdom-global": "3.0.2",
    "lodash-es": "4.17.21",
    "luxon": "1.26.0",
    "microtime": "3.0.0",
    "prettier": "2.2.1",
    "puppeteer": "8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "2.42.3",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-terser-js": "1.0.2",
    "routes": "2.1.0",
    "sinon": "10.0.0",
    "sinon-chai": "3.6.0",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "tsd-jsdoc": "2.5.0",
    "tslib": "2.1.0",
    "tslint-config-prettier": "1.18.0",
    "typedoc": "0.20.34",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "@hookform/error-message": "^0.0.5",
    "create-react-class": "^15.7.0",
    "memory-cache": "0.2.0",
    "numeral": "2.0.6",
    "react-dom-factories": "1.0.2",
    "react-hook-form": "^6.15.5",
    "ua-parser-js": "^0.7.24",
    "use-global-hook": "^0.2.1"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "Release: ${nextRelease.version}\n\n${nextRelease.notes}"
      }
    ],
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/changelog"
    ]
  }
}
